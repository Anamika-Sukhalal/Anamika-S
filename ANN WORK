        "#         self.conv3 = nn.Conv2d(32, 16, 2) # 32*6*6 X 2*2 ==> 16*4*4\n",
        "#         self.fc1 = nn.Linear(16 * 2 * 2, 240) # 16*5*5 ==> 120\n",
        "#         self.fc2 = nn.Linear(240, 120) # 16*5*5 ==> 120\n",
        "#         self.fc3 = nn.Linear(120, 84) # 120 ==> 84\n",
        "#         self.fc4 = nn.Linear(84, 10) # 84 ==> 10\n",
